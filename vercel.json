{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "ignoreCommand": "echo frontend-only",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
      }
    ]
  }
  